{
  "name": "invoice-analytics-app",
  "version": "1.0.0",
  "description": "Web application for invoice management and analytics for Contract Specialists",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^13.5.0",
    "helmet": "^7.1.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.13.0",
    "tesseract.js": "^4.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "invoice",
    "analytics",
    "contracts",
    "pdf",
    "ocr",
    "firebase"
  ],
  "author": "Contract Specialist Team",
  "license": "MIT"
}
